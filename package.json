{
  "name": "purupdater",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "ng": "ng build && xcopy /EY dist public",
    "s": "npm run ng && xcopy /EY electron public && electron public",
    "deploy": "npm run ng && electron-packager public PurUpdater --platform=win32 --arch=ia32 --icon=public/favicon.ico --overwrite --app-version=\"1.0.0\" --win32metadata.CompanyName=\"Impeo Software GmbH\" --win32metadata.ProductName=\"PurUpdater\" --win32metadata.OriginalFilename=\"PurUpdater.exe\" --win32metadata.FileDescription=\"PurUpdater\" --app-copyright=\"Copyright (C) 2016 Impeo Software GmbH. All rights reserved.\" --asar --out=\"deploy\""
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "decompress-zip": "^0.3.0",    
    "fs-extra": "^1.0.0",
    "jquery": "^3.1.1",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.30",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.19-3",
    "codelyzer": "1.0.0-beta.1",
    "electron": "^1.4.7",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "~2.0.3",
    "webdriver-manager": "10.2.5"
  }
}
